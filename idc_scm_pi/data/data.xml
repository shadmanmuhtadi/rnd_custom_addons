<odoo>
    <data>
        <record id="seq_purchase_order" model="ir.sequence">
            <field name="name">Purchase Orders</field>
            <field name="code">purchase.po</field>
            <field name="prefix">PO/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_proforma_invoice" model="ir.sequence">
            <field name="name">Proforma Invoice</field>
            <field name="code">purchase.pi</field>
            <field name="prefix">PI/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>