<odoo>
    <data>

        <!-- Sequences for purchase.order.po -->
        <record id="seq_purchase_order" model="ir.sequence">
            <field name="name">Purchase Order</field>
            <field name="code">purchase.po</field>
            <field name="prefix">PO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for purchase.order.pi -->
        <record id="seq_purchase_order_pi" model="ir.sequence">
            <field name="name">Purchase Order pi</field>
            <field name="code">purchase.pi</field>
            <field name="prefix">PI</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- sequence for custom reference -->
        <record id="seq_form_serial_no" model="ir.sequence">
          <field name="name">Sequence for custom serial number</field>
          <field name="code">purchase.po.customized.sequence</field>
          <field name="prefix"></field>
          <field name="padding">7</field>
          <field name="company_id" eval="False"/>
         </record>
    </data>
</odoo>